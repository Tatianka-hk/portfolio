{
  "env": {
      "node": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended"
  ],
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 11
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      // "quotes": [
      //     "error",
      //     "double"
      // ],
      // "semi": [
      //     "error",
      //     "always"
      // ],
      "no-console": [
          "warn"
      ],
      "no-unused-vars":[
          "warn"
      ]
  }
}